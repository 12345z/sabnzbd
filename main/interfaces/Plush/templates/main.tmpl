<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"/>
<html>
<head>

	<title>SABnzbd+ Plush</title>
	
	<link rel="shortcut icon" type="image/ico" href="static/images/plush_default/sabnzbdplus.ico"/>
	<link rel="alternate" type="application/rss+xml" title="RSS 2.0" href="rss?mode=history"/>
	<link rel="stylesheet" type="text/css" href="static/stylesheets/plush-default.css" media="screen"/>
    <!--#if $color_scheme#-->
    <link rel="stylesheet" type="text/css" href="static/stylesheets/colorschemes/$color_scheme"/>
    <!--#end if#-->

	<script type="text/javascript" src="static/javascripts/jquery.pack.js"></script>
	<script type="text/javascript" src="static/javascripts/superfish.js"></script>
	<script type="text/javascript" src="static/javascripts/webtoolkit.aim.js"></script>
	<script type="text/javascript" src="static/javascripts/jquery.tablednd.js"></script>
	<script type="text/javascript" src="static/javascripts/plush.js"></script>

</head>
<body>

<!--#if $new_release#-->
<div id="new_release_notification">&nbsp;<a href="$new_rel_url" target="_blank">New release $new_release available!</a>&nbsp;</div>
<!--#end if#-->

	<div id="page-wrap">
		<div id="hdr-queue">
                        <div class="logo">
				<h1>Queue</h1>
			</div>

			<div id="queue-buttons">
				<ul>
					<li class="q_menu_sort" title="Sort Alphabetically &mdash; click again to Sort by Average File Age"></li>
					<!-- <li class="q_menu_verbose"></li> -->
					<li class="q_menu_purge" title="Purge Queue Contents (double-click)"></li>
					<li class="q_menu_pause<!--#if $paused#--> q_menu_paused<!--#end if#-->" title="Pause/Resume the Queue" id="pause_resume"></li>
				</ul>
			</div>
			<div id="time-left">
				<p><span id="stats_eta">&hellip;</span>&nbsp;left</p>
			</div>
			<ul class="nav">
		<li><a class="queue_logo"></a>
			<ul>
				<li><a>Add NZB</a>
					<ul>
						<li><a>&raquo; URL<!--#if $varExists('newzbinDetails') then "&nbsp;&amp;&nbsp;Newzbin" else ""#--></a>
							<ul>
								<li><a><input type="text" id="addID_input" value="enter URL/ID" size="13"  onfocus="if(value=='enter URL/ID')value=''" onblur="if(!value)value='enter URL/ID'"/></a></li>
								<li><a>&raquo;&nbsp;<input type="submit" id="addID" value="Add NZB" style="cursor:pointer"/></a></li>
								<li>
									<!--#if $cat_list#-->
        							<a><select id="addID_cat" title="Category">
							        <!--#for $ct in $cat_list#-->
							        	<option value="$ct">$ct</option>
							        <!--#end for#-->
							        </select></a>
							        <!--#end if#-->
							        <a><select id="addID_pp" title="Post-Processing Option">
							        	<option value="-1" <!--#if $cat_list then "selected" else ""#-->>(Default)</option>
							        	<option value="0">(Skip)</option>
							        	<option value="1">+Repair</option>
							        	<option value="2">+Unpack</option>
							        	<option value="3" <!--#if $cat_list then "" else "selected"#-->>+Delete</option>
							        </select></a>
							        <!--#if $script_list#-->
							        <a><select id="addID_script" title="Script">
							        <!--#for $sc in $script_list#-->
							        	<option value="$sc">$sc</option>
							        <!--#end for#-->
							        </select></a>
							        <!--#end if#-->
								</li>
							</ul>
						</li>
						<li><a>&raquo; File Upload</a>
							<form action="addFile" method="post" enctype="multipart/form-data" onsubmit="return AIM.submit(this, {'onStart': startCallback, 'onComplete': completeCallback})">
							<ul>
								<li><a><input type="file" name="nzbfile" title="Enqueue nzb by File Upload" size="4" /></a></li>
								<li><a>&raquo;&nbsp;<input type="submit" id="addNZBbyFile" value="Add NZB" style="cursor:pointer"/></a></li>
								<li>
						        <!--#if $cat_list#-->
        				   		<a><select name="cat" title="Category">
						        <!--#for $ct in $cat_list#-->
						        	<option value="$ct">$ct</option>
						        <!--#end for#-->
						        </select></a>
						        <!--#end if#-->
						        <a><select name="pp" title="Post-Processing Option">
						        <option value="-1" <!--#if $cat_list then "selected" else ""#-->>(Default)</option>
						        <option value="0">(Skip)</option>
						        <option value="1">+Repair</option>
						        <option value="2">+Unpack</option>
						        <option value="3" <!--#if $cat_list then "" else "selected"#-->>+Delete</option>
						        </select></a>
						        <!--#if $script_list#-->
						        <a><select name="script" title="Script">
						        <!--#for $sc in $script_list#-->
						        	<option value="$sc">$sc</option>
						        <!--#end for#-->
						        </select></a>
						        <!--#end if#-->
								</li>
							</ul>
							</form>
						</li>
					</ul>
				</li>
				<li><a><select id="refreshRate-option">
							<option value="1">1 sec</option>
							<option value="2">2 sec</option>
							<option value="4">4 sec</option>
							<option value="8">8 sec</option>
							<option value="16">16 sec</option>
							<option value="30">30 sec</option>
							<option value="60">1 min</option>
							<option value="120">2 min</option>
							<option value="900">15 min</option>
							<option value="1800">30 min</option>
							<option value="0">No</option>
						</select>&nbsp;Refresh</a></li>
				<li><a>On Queue Finish</a>
					<ul><li>
						<a><select style="width:130px" id="onQueueFinish-option">
							<option value=""></option>
							    <optgroup label="Actions">
							        <!--#if $nt#-->
							        <option value="shutdown_pc" <!--#if $finishaction == 'shutdown_pc' then 'selected' else ''#-->>Shutdown PC</option>
							        <option value="hibernate_pc" <!--#if $finishaction == 'hibernate_pc' then 'selected' else ''#-->>Hibernate PC</option>
							        <!--#end if#-->
							        <option value="shutdown_program" <!--#if $finishaction == 'shutdown_program' then 'selected' else ''#-->>Shutdown SABnzbd</option>
							    </optgroup>
							<!--#if $script_list#-->
							    <optgroup label="Scripts">
							    <!--#for $sc in $script_list#-->
							        <!--#if $sc != 'None'#-->
							        <option value="script_$sc" <!--#if $finishaction == 'script_'+$sc then 'selected' else ''#-->>$sc</option>
							        <!--#end if#-->
							    <!--#end for#-->
							    </optgroup>
							<!--#end if#-->
						</select></a>
					</li></ul>
				</li>
				<li><a href="connections">Configuration</a>
					<ul>
						<li><a href="connections">Connections</a></li>
						<li><a href="config/general">General</a></li>
						<li><a href="config/directories">Directories</a></li>
						<li><a href="config/switches">Switches</a></li>
						<li><a href="config/server">Servers</a></li>
						<li><a href="config/categories">Categories</a></li>
						<li><a href="config/scheduling">Scheduling</a></li>
						<li><a href="config/rss">RSS</a></li>
						<li><a href="config/email">Email</a></li>
						<li><a href="config/newzbin">Newzbin</a></li>
					</ul>
				</li>
				<li><a href="$helpuri" target="_blank">Help / Wiki</a></li>
				<li><a href="http://forums.sabnzbd.org/" target="_blank"><small>forums.sabnzbd.org</small></a></li>
				<li><a style="cursor:pointer" onclick="if(confirm('Sure you want to shut down the SABnzbd application?')){window.location='shutdown';}"><small>Shutdown SABnzbd</small></a></li>
			</ul>
		</li>
	</ul>
			<div id="speed">
				<p><span id="stats_kbpersec">&hellip;</span>&nbsp;kB/s</p>
			</div>
			
			<!-- <div class="logo">
				<h1>Queue</h1>
			</div> -->
			
		</div>

		<div id="queue-wrap">

			<!-- Queue -->
			<div id="queue"></div>
		
		
			<!-- History Separator -->
			<div class="box_banner_history">
				<div class="box_banner_r">
					<ul class="box_banner_menu">
						<li class="h_menu_verbose" title="Toggle History Verbosity"></li>
						<li class="h_menu_purge" title="Purge History Contents &mdash; NOT active NZBs (double-click)"></li>
					</ul>
				</div>
			</div>
			<div class="box_banner_hr"></div>
		
		
			<!-- History -->
			<div id="history"></div>
		
		</div>
		
		
		<div id="btm-wrapper">
			<div class="corner-right"></div>
			<div class="corner-left"></div>
		</div>
	</div>

<!--#if $warning#-->
    <h2>WARNING</h2>
    <b>$warning</b>
<!--#end if#-->

</body>
</html>
