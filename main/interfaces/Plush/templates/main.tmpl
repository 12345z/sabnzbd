<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"/>
<html>
<head>

    <title>SABnzbd+ Plush</title>
    <link rel="alternate" type="application/rss+xml" title="RSS 2.0" href="rss?mode=history"/>
    <link rel="shortcut icon" type="image/ico" href="static/images/plush-default/sabnzbdplus.ico"/>

    <link rel="stylesheet" type="text/css" href="static/stylesheets/plush-default.css?$version"/>
    <!--#if $color_scheme and $color_scheme != "plush-default.css" and $color_scheme != "default.css"#-->
    <link rel="stylesheet" type="text/css" href="static/stylesheets/colorschemes/$color_scheme?$version"/>
    <!--#end if#-->

    <script type="text/javascript" src="static/javascripts/plush.js?$version"></script>

	<script type="text/javascript">
	    jQuery(function($){
        	$.plush.apikey = "$session";
        	$.plush.Tprev = "$T('Plush-prev')"; // translations for pagination
        	$.plush.Tnext = "$T('Plush-next')";
        	$.plush.Tconfirmation = "$T('Plush-confirm')";
		});
    </script>

</head>
<body>

	<div id="main_menu">
    <ul class="sf-menu sf-navbar">
        <li>
            <a class="sf-with-ul">SABnzbd</a>
            <ul>
                <li id="plushoptions"><a>$T('Plush-options')</a>
                	<ul>
                		<li><a>$T('Plush-refreshRate'):&nbsp;<select id="refreshRate-option">
	                        <option value="1">1 $T('second')</option>
	                        <option value="2">2 $T('second')</option>
	                        <option value="4">4 $T('second')</option>
	                        <option value="8">8 $T('second')</option>
	                        <option value="16">16 $T('second')</option>
	                        <option value="30">30 $T('second')</option>
	                        <option value="60">1 $T('minute')</option>
	                        <option value="120">2 $T('minute')</option>
	                        <option value="900">15 $T('minute')</option>
	                        <option value="1800">30 $T('minute')</option>
	                        <option value="0">$T('Plush-disabled')</option>
                    	</select></a></li>
                    	<li><a><input type="checkbox" id="confirmDeleteQueue" checked /> $T("Plush-confirmDeleteQueue")</a></li>
                    	<li><a><input type="checkbox" id="confirmDeleteHistory" checked /> $T("Plush-confirmDeleteHistory")</a></li>
                    	<li id="explain-blockRefresh" title="$T("Plush-explain-blockRefresh")"><a><input type="checkbox" id="blockRefresh" checked /> $T("Plush-blockRefresh")</a></li>
                    </ul>
                </li>
                <li><a class="sf-with-ul">$T('menu-help')</a>
                    <ul>
                        <li><a href="$helpuri" target="_blank">$T('menu-wiki')</a></li>
                        <li><a href="http://forums.sabnzbd.org" target="_blank">$T('menu-forums')</a></li>
                        <li><a href="http://www.sabnzbd.org/live-chat/" target="_blank">$T('menu-irc')</a></li>
                    </ul>
                </li>
                <li><a class="pointer" id="shutdown_sabnzbd" rel="$T('shutdownOK?')">$T('sch-shutdown')</a></li>
            </ul>
        </li>
        <li>
            <a class="sf-with-ul">$T('Plush-addnzb')</a>
            <ul>
                <li><a><form><input type="text" id="addID_input" size="18" /><input type="submit" id="addID" value="$T('Plush-fetch')" /></form></a></li>
                <form action="tapi" id="uploadNZBForm" method="post" enctype="multipart/form-data">
				          <input type="hidden" name="apikey" value="$session" />
                    <li>
                      <a id="uploadTip" title="$T('Plush-uploadTip')">
            					<input type="hidden" name="mode" value="addfile" />
            					<input type="file" id="uploadNZBFile" name="name" size="10" />
              				</a>
            				</li>
                    <li id="processingContainer">
                      <a class="sf-with-ul">$T('pp')</a>
                      <ul>
                        <!--#if $cat_list#-->
                        <li><label>$T('category'):</label>
                            <select id="addID_cat" name="cat">
                            <!--#for $ct in $cat_list#-->
                                <option value="$ct">$Tspec($ct)</option>
                            <!--#end for#-->
                            </select>
                        </li>
                        <!--#end if#-->
                        <li><label>$T('priority'):</label>
                            <select id="addID_priority" name="priority">
                                <option value="-100" selected>$T('default')</option>
                                <option value="2">$T('pr-force')</option>
                                <option value="1">$T('pr-high')</option>
                                <option value="0">$T('pr-normal')</option>
                                <option value="-1">$T('pr-low')</option>
                            </select>
                        </li>
                        <li><label>$T('pp'):</label>
                            <select id="addID_pp" name="pp">
                                <option value="-1">$T('default')</option>
                                <option value="0">$T('pp-none')</option>
                                <option value="1">$T('pp-repair')</option>
                                <option value="2">$T('pp-unpack')</option>
                                <option value="3">$T('pp-delete')</option>
                            </select>
                        </li>
                        <!--#if $script_list#-->
                        <li><label>$T('script'):</label>
                            <select id="addID_script" name="script">
                            <!--#for $sc in $script_list#-->
                                <option value="$sc">$Tspec($sc)</option>
                            <!--#end for#-->
                            </select>
                        </a></li>
                        <!--#end if#-->
                      </ul>
                    </li>
				<!--#if $varExists('newzbinDetails')#-->
                <li><a title="Newzbin - $T('link-getBookmarks')" id="fetch_newzbin_bookmarks" class="pointer">$T('link-getBookmarks')</a></li>
          				<!--#end if#-->
          				</form>
            </ul>
        </li>
        <li>
            <a class="sf-with-ul">$T('menu-queue')</a>
            <ul>
                <li><a>$T('Plush-maxSpeed'):&nbsp;&nbsp;<input type="text" id="maxSpeed-option" size="6" />&nbsp;&nbsp;$T('KBs')</a></li>
                <li>
                    <a class="sf-with-ul">$T('Plush-onQueueFinish'):
                        <select id="onQueueFinish-option">
                            <option value=""></option>
                            <optgroup label="$T('eoq-actions')">
                                <option value="shutdown_program" <!--#if $finishaction == 'shutdown_program' then 'selected' else ''#-->>$T('shutdownSab')</option>
                                <!--#if $power_options#-->
                                <option value="shutdown_pc" <!--#if $finishaction == 'shutdown_pc' then 'selected' else ''#-->>$T('shutdownPc')</option>
                                <option value="standby_pc" <!--#if $finishaction == 'standby_pc' then 'selected' else ''#-->>$T('standbyPc')</option>
                                <option value="hibernate_pc" <!--#if $finishaction == 'hibernate_pc' then 'selected' else ''#-->>$T('hibernatePc')</option>
                                <!--#end if#-->
                            </optgroup>
                            <!--#if $script_list#-->
                            <optgroup label="$T('eoq-scripts')">
                                <!--#for $sc in $script_list#-->
                                <!--#if $sc != 'None'#-->
                                <option value="script_$sc" <!--#if $finishaction == 'script_'+$sc then 'selected' else ''#-->>$sc</option>
                                <!--#end if#-->
                                <!--#end for#-->
                            </optgroup>
                            <!--#end if#-->
                        </select>
                    </a>
                </li>
                <li>
                    <a class="sf-with-ul">$T('Plush-sort')</a>
                    <ul id="queue_sort_list">
                        <li><a class="queue_sort pointer" rel="avg_age&dir=asc">$T('Plush-sortAgeAsc')</a></li>
                        <li><a class="queue_sort pointer" rel="avg_age&dir=desc">$T('Plush-sortAgeDesc')</a></li>
                        <li><a class="queue_sort pointer" rel="name&dir=asc">$T('Plush-sortNameAsc')</a></li>
                        <li><a class="queue_sort pointer" rel="name&dir=desc">$T('Plush-sortNameDesc')</a></li>
                        <li><a class="queue_sort pointer" rel="size&dir=asc">$T('Plush-sortSizeAsc')</a></li>
                        <li><a class="queue_sort pointer" rel="size&dir=desc">$T('Plush-sortSizeDesc')</a></li>
                    </ul>
                </li>
                <li><a id="queue_purge" class="pointer" rel="$T('Plush-confirmPurgeQ')">$T('Plush-purge')</a></li>
            </ul>
        </li>
        <li>
  			  <a class="sf-with-ul" href="connections" id="last_warning">$T('menu-config')&nbsp;<small id="have_warnings">($have_warnings)</small></a>
      		<ul class="config_links">
            <li><a href="connections">$T('menu-cons')</a></li>
            <li><a href="config/general">$T('cmenu-general')</a></li>
            <li><a href="config/directories">$T('cmenu-folders')</a></li>
            <li><a href="config/switches">$T('cmenu-switches')</a></li>
            <li><a href="config/server">$T('cmenu-servers')</a></li>
            <li><a href="config/scheduling">$T('cmenu-scheduling')</a></li>
            <li><a href="config/rss">$T('cmenu-rss')</a></li>
            <li><a href="config/email">$T('cmenu-email')</a></li>
            <li><a href="config/newzbin">$T('cmenu-newzbin')</a></li>
            <li><a href="config/categories">$T('cmenu-cat')</a></li>
            <li><a href="config/sorting">$T('cmenu-sorting')</a></li>
      		</ul>
      	</li>
        <li><a id="manual_refresh_wrapper">$T('Plush-button-refresh')</a></li>
	    <!--#if $new_release#-->
        <li><a href="$new_rel_url" id="new_release" target="_blank">$T('Plush-updateAvailable').replace(' ','&nbsp;')</a></li>
	    <!--#end if#-->
	    <!--#if $warning#-->
        <li><a id="warning_message">$warning.replace(' ','&nbsp;')</a></li>
	    <!--#end if#-->
    </ul>
	</div>


    <div id="page-wrap">

        <div id="hdr-queue">
            <div class="logo main_sprite_container sprite_topleft_queue"><h1>$T('menu-queue').upper()</h1></div>
            <div class="main_sprite_container sprite_topright_queue" id="queue-buttons">
                <ul>
                    <li class="main_sprite_container sprite_q_menu_pausefor">
                      <a href="#">Pause for...</a>
                      <ul id="set_pause_list">
                          <li><a href="javascript:void(0)" class="set_pause pointer" rel="5">$T('Plush-pause5m')</a></li>
                          <li><a href="javascript:void(0)" class="set_pause pointer" rel="15">$T('Plush-pause15m')</a></li>
                          <li><a href="javascript:void(0)" class="set_pause pointer" rel="30">$T('Plush-pause30m')</a></li>
                          <li><a href="javascript:void(0)" class="set_pause pointer" rel="60">$T('Plush-pause1h')</a></li>
                          <li><a href="javascript:void(0)" class="set_pause pointer" rel="180">$T('Plush-pause3h')</a></li>
                          <li><a href="javascript:void(0)" class="set_pause pointer" rel="360">$T('Plush-pause6h')</a></li>
                          <li><a href="javascript:void(0)" class="set_pause pointer" rel="custom" title="$T('Plush-pauseForPrompt')">$T('Plush-pauseFor')</a></li>
                      </ul>
                    </li>
                    <li class="main_sprite_container <!--#if $paused then 'sprite_q_pause_on' else 'sprite_q_pause'#-->" title="$T('link-pause')&nbsp;&frasl;&nbsp;$T('link-resume')" id="pause_resume">Pause/Resume</li>
					<li id="pause_int"></li>
                </ul>
            </div>
            <div class="centerpiece">
                <div class="main_sprite_container sprite_rounded_area_bg" id="speed"><p><span id="stats_speed">&hellip;</span></p></div>
                <div id="queue_logo" class="main_sprite_container sprite_sabnzbdplus_logo"></div>
                <div class="main_sprite_container sprite_rounded_area_bg" id="time-left"><p><span id="stats_eta">&hellip;</span>&nbsp;$T('Plush-left')</p></div>
            </div>
        </div>

        <div id="queue-wrap">

            <!-- Queue ( queue.tmpl )-->
            <div id="queue">
                <br/><p class="loading">&nbsp;</p>
                <input type="hidden" id="queue_view_preference" value="" />
            </div>
			<div class="box_fatbottom" id="box_fatbottom_queue">
			    <div class="right_stats" id="queue_stats" nowrap></div>
			    <div class="left_stats">
			    	<select id="queue-pagination-perpage" class="pagination-perpage">
			    		<option value="5">5 / $T('Plush-page')</option>
			    		<option value="10">10 / $T('Plush-page')</option>
			    		<option value="20">20 / $T('Plush-page')</option>
			    		<option value="30">30 / $T('Plush-page')</option>
			    		<option value="40">40 / $T('Plush-page')</option>
			    		<option value="50">50 / $T('Plush-page')</option>
			    		<option value="9999999">$T('Plush-everything')</option>
			    	</select>
			    	<div id="queue-pagination" class="pagination"></div>
			    </div>
			</div>

            <!-- History Menu/Banner -->
            <div class="box_banner_history">
				<h1>$T('menu-history').upper()</h1>
                <div class="box_banner_r">
                    <ul class="box_banner_menu">
                        <li class="main_sprite_container sprite_h_purge show" id="hist_purge" title="$T('purgeHist')" rel="$T('Plush-confirmPurgeH')"></li>
                    </ul>
                </div>
            </div>
            <div class="box_banner_hr"></div>

            <!-- History ( history.tmpl ) -->
            <div id="history">
                <br/><p class="loading">&nbsp;</p>
                <input type="hidden" id="history_view_preference" value="" />
            </div>
			<div class="box_fatbottom history_stats">
			    <div class="right_stats" id="history_stats" nowrap></div>
			    <div class="left_stats">
			    	<select id="history-pagination-perpage" class="pagination-perpage">
			    		<option value="5">5 / $T('Plush-page')</option>
			    		<option value="10">10 / $T('Plush-page')</option>
			    		<option value="20">20 / $T('Plush-page')</option>
			    		<option value="30">30 / $T('Plush-page')</option>
			    		<option value="40">40 / $T('Plush-page')</option>
			    		<option value="50">50 / $T('Plush-page')</option>
			    		<option value="9999999">$T('Plush-everything')</option>
			    	</select>
			    	<div id="history-pagination" class="pagination"></div>
			    </div>
			    <div class="plush_wrapper"><div class="main_sprite_container sprite_sabnzbdplus_plush">&nbsp;</div></div>
			</div>

        </div>

        <div id="btm-wrapper">
            <div class="main_sprite_container sprite_btm_corner_right"></div>
            <div class="main_sprite_container sprite_btm_corner_left"></div>
        </div>

    </div>

</body>
</html>
