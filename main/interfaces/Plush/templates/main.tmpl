<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"/>
<html>
<head>

    <title>SABnzbd+ Plush</title>
    <link rel="alternate" type="application/rss+xml" title="RSS 2.0" href="rss?mode=history"/>
    <link rel="shortcut icon" type="image/ico" href="static/images/plush-default/sabnzbdplus.ico"/>

    <link rel="stylesheet" type="text/css" href="static/stylesheets/plush-default.css?$version"/>
    <!--#if $color_scheme and $color_scheme != "plush-default.css" and $color_scheme != "default.css"#-->
    <link rel="stylesheet" type="text/css" href="static/stylesheets/colorschemes/$color_scheme?$version"/>
    <!--#end if#-->

    <script type="text/javascript" src="static/javascripts/main.js?$version"></script>

</head>
<body>

    <ul class="sf-menu sf-navbar">
        <li>
            <a class="sf-with-ul">SABnzbd</a>
            <ul>
                <li><a>Max&nbsp;Speed:&nbsp;<input type=text id="maxSpeed-option" size="6" /><small>&nbsp;KB/s</small></a></li>
                <li><a>Refresh&nbsp;Rate:&nbsp;<select id="refreshRate-option">
                        <option value="1">1 second</option>
                        <option value="2">2 seconds</option>
                        <option value="4">4 seconds</option>
                        <option value="8">8 seconds</option>
                        <option value="16">16 seconds</option>
                        <option value="30">30 seconds</option>
                        <option value="60">1 minute</option>
                        <option value="120">2 minutes</option>
                        <option value="900">15 minutes</option>
                        <option value="1800">30 minutes</option>
                        <option value="0">Disabled</option>
                    </select></a>
                </li>
                <li><a class="sf-with-ul">Help</a>
                    <ul>
                        <li><a href="$helpuri" target="_blank" title="Open Official Manual" class="tip">Wiki</a></li>
                        <li><a href="http://forums.sabnzbd.org" target="_blank" title="Open Official Message Board" class="tip">Forums</a></li>
                        <li title="Load IRC Client<br/><small>irc.synirc.net #sabnzbd</small>" class="tip"><a class="pointer" id="help_irc" href="http://www.sabnzbd.org/live-chat/" target="_blank">Live Chat</a></li>
                    </ul>
                </li>
                <li><a class="pointer" id="shutdown_sabnzbd">Shutdown</a></li>
            </ul>
        </li>
        <li>
            <a class="sf-with-ul">Add NZB</a>
            <ul>
                <li><a><form><input type="text" id="addID_input" size="20" /><input type="submit" id="addID" value="Fetch" /></form></a></li>
                <li><a><div id="addNZBbyFile"><input class="pointer" type="submit" value="Upload" /></div></a></li>
                <li>
                    <a>Processing</a>
                    <ul>
                    <li><a>Priority:
                        <select id="addID_priority" name="priority">
                            <option value="2">Force</option>
                            <option value="1">High</option>
                            <option value="0" selected>Normal</option>
                            <option value="-1">Low</option>
                        </select>
                    </a></li>
                    <!--#if $cat_list#-->
                    <li><a>Category:
                        <select id="addID_cat" name="cat">
                        <!--#for $ct in $cat_list#-->
                            <option value="$ct">$ct</option>
                        <!--#end for#-->
                        </select>
                    </a></li>
                    <!--#end if#-->
                    <li><a>Processing:
                        <select id="addID_pp" name="pp">
                            <option value="-1">Default</option>
                            <option value="0">Skip</option>
                            <option value="1">+Repair</option>
                            <option value="2">+R+Unpack</option>
                            <option value="3">+R+U+Delete</option>
                        </select>
                    </a></li>
                    <!--#if $script_list#-->
                    <li><a>Script:
                        <select id="addID_script" name="script">
                        <!--#for $sc in $script_list#-->
                            <option value="$sc">$sc</option>
                        <!--#end for#-->
                        </select>
                    <!--#end if#-->
                    </a></li>
                    </ul>
                </li>
				<!--#if $varExists('newzbinDetails')#-->
                <li><a><input title="Fetch Newzbin Bookmarks" id="fetch_newzbin_bookmarks" class="tip pointer" type="submit" value="Bookmarks" /></a></li>
				<!--#end if#-->
            </ul>
        </li>
        <li>
            <a class="sf-with-ul">Queue</a>
            <ul>
                <li>
                    <a class="sf-with-ul">Upon Completion:
                        <select id="onQueueFinish-option">
                            <option value="">Do Nothing</option>
                            <optgroup label="Actions">
                                <option value="shutdown_program" <!--#if $finishaction == 'shutdown_program' then 'selected' else ''#-->>Shutdown SABnzbd</option>
                                <!--#if $nt or $darwin#-->
                                <option value="shutdown_pc" <!--#if $finishaction == 'shutdown_pc' then 'selected' else ''#-->>Shutdown PC</option>
                                <option value="standby_pc" <!--#if $finishaction == 'standby_pc' then 'selected' else ''#-->>Standby PC</option>
                                <!--#end if#-->
                                <!--#if $nt#-->
                                <option value="hibernate_pc" <!--#if $finishaction == 'hibernate_pc' then 'selected' else ''#-->>Hibernate PC</option>
                                <!--#end if#-->
                            </optgroup>
                            <!--#if $script_list#-->
                            <optgroup label="Scripts">
                                <!--#for $sc in $script_list#-->
                                <!--#if $sc != 'None'#-->
                                <option value="script_$sc" <!--#if $finishaction == 'script_'+$sc then 'selected' else ''#-->>$sc</option>
                                <!--#end if#-->
                                <!--#end for#-->
                            </optgroup>
                            <!--#end if#-->
                        </select>
                    </a>
                </li>
                <li>
                    <a class="sf-with-ul">Sort</a>
                    <ul>
                        <li><a class="pointer" id="sort_by_avg_age_asc">by Average Age <small>[Oldest&rarr;Newest]</small></a></li>
                        <li><a class="pointer" id="sort_by_avg_age_desc">by Average Age <small>[Newest&rarr;Oldest]</small></a></li>
                        <li><a class="pointer" id="sort_by_name_asc">by Name <small>[A&rarr;Z]</small></a></li>
                        <li><a class="pointer" id="sort_by_name_desc">by Name <small>[Z&rarr;A]</small></a></li>
                        <li><a class="pointer" id="sort_by_size_asc">by Size <small>[Smallest&rarr;Largest]</small></a></li>
                        <li><a class="pointer" id="sort_by_size_desc">by Size <small>[Largest&rarr;Smallest]</small></a></li>
                    </ul>
                </li>
                <li><a id="queue_purge" class="pointer">Purge</a></li>
                <li><a class="tip" title="Toggle Queue Priority Visibility"><input type="checkbox" id="priority-toggle" /> Priorities</a></li>
            </ul>
        </li>
        <li>
            <a class="sf-with-ul" href="connections">Warnings&nbsp;<small id="have_warnings">($have_warnings)</small></a>
            <ul>
                <li><a href="connections" id="last_warning_link">Latest:&nbsp;<span id="last_warning"><!--#if $have_warnings == "0" #-->nothing to report<!--#end if#-->$last_warning</span></a></li>
            </ul>
        </li>
        <li><a href="connections">Config</a></li>
        <li><a class="pointer" id="manual_refresh_wrapper"><span id="manual_refresh">Refresh</span></a></li>
	    <!--#if $new_release#-->
        <li><a href="$new_rel_url" id="new_release" target="_blank">New&nbsp;release&nbsp;$new_release&nbsp;available!</a></li>
	    <!--#end if#-->
	    <!--#if $warning#-->
        <li><a id="warning_message">$warning</a></li>
	    <!--#end if#-->
    </ul>



    <div id="page-wrap">

        <div id="hdr-queue">

            <div class="logo">
                <h1>Queue</h1>
            </div>

            <div id="queue-buttons">
                <ul>
                    <li class="tip q_menu_pause <!--#if $paused then 'q_menu_paused' else 'q_menu_unpaused'#-->" title="Pause&nbsp;&frasl;&nbsp;Resume" id="pause_resume"></li>
                </ul>
            </div>

            <div class="centerpiece">

                <div id="speed">
                    <p><span id="stats_kbpersec">&hellip;</span>&nbsp;KB/s</p>
                </div>

                <div id="queue_logo"></div>

                <div id="time-left">
                    <p><span id="stats_eta">&hellip;</span>&nbsp;left</p>
                </div>

            </div>
        </div>

        <div id="queue-wrap">

            <!-- Queue ( queue.tmpl )-->
            <div id="queue">
                <p class="loading">&nbsp;</p>
                <input type="hidden" id="queue_view_preference" value="" />
            </div>

            <!-- History Menu/Banner -->
            <div class="box_banner_history">
                <div class="box_banner_r">
                    <ul class="box_banner_menu">
                        <li class="h_menu_purge show tip" title="Purge&nbsp;History&nbsp;Listings"></li>
                    </ul>
                </div>
            </div>
            <div class="box_banner_hr"></div>

            <!-- History ( history.tmpl ) -->
            <div id="history">
                <p class="loading">&nbsp;</p>
                <input type="hidden" id="history_view_preference" value="" />
            </div>
			<div class="box_fatbottom">
			    <div class="right_stats" id="history_stats" nowrap></div>
			    <div class="left_stats">
			    	<div id="history-pagination" class="pagination"></div>
			    	<a name="p"></a>
			    </div>
			    <div class="history_logo">&nbsp;</div>
			</div>

        </div>

        <div id="btm-wrapper">
            <div class="corner-right"></div>
            <div class="corner-left"></div>
        </div>

    </div>

</body>
</html>
