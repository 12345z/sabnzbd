<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN">
<html>
<head>
	<title>SABnzbd+ $version - Queued: $mbleft MB</title>
	<link rel="shortcut icon" type="image/ico" href="../../static/images/plush-default/sabnzbdplus.ico"/>
	
	<script type="text/javascript" src="../../static/javascripts/config.js?$version"></script>
	
	<link rel="stylesheet" type="text/css" href="../../static/stylesheets/plush-default.css?$version"/>
	<!--#if $color_scheme and $color_scheme != "plush-default.css" and $color_scheme != "plush_default.css" and $color_scheme != "default.css" and $color_scheme != "modern.css" and $color_scheme != "Modern.css"#-->
	<link rel="stylesheet" type="text/css" href="../../static/stylesheets/colorschemes/$color_scheme?$version"/>
	<!--#end if#-->
	
	<script language="javascript">
		var config_pane = "NZO"; // re-uses config's js/css
	</script>
	
</head>
<body class="nzo">
	
	<div align="center">
		<a href="../../" style="text-decoration:none; color:black">&laquo; go back <img src="../../static/images/plush-default/sabnzbdplus_logo.png" border="0"/></a>
	</div>
	
	<form action="bulk_operation">
	
	<div id="config_nav">
		<ul>
			<li><a class="config_menu" id="config_nzo_reload" onclick="document.location.reload()">Reload</a></li>
			<br/>
			<li><a class="config_menu" id="config_nzo_delete"><input type="submit" name="action_key" value="Delete" /></a></li>
			<li><a class="config_menu" id="config_nzo_top"><input type="submit" name="action_key" value="Top" /></a></li>
			<li><a class="config_menu" id="config_nzo_up"><input type="submit" name="action_key" value="Up" /></a></li>
			<li><a class="config_menu" id="config_nzo_down"><input type="submit" name="action_key" value="Down" /></a></li>
			<li><a class="config_menu" id="config_nzo_bottom"><input type="submit" name="action_key" value="Bottom" /></a></li>
			<br/>Selection:
			<li><a class="config_menu" id="config_nzo_select_all">ALL</a></li>
			<li><a class="config_menu" id="config_nzo_select_none">NONE</a></li>
			<li><a class="config_menu" id="config_nzo_select_invert">INVERT</a></li>
		</ul>
	</div>
	
	<div id="config_container" class="config_margin">
		<div id="config_content">
			
	<!-- ************************************
				PAGE CONTENT IS BELOW
		************************************ -->
			
			<table class="nzoTable">
				<tr><th></th><th>Filename/Subject</th><th>Size (MB)</th><th>Age</th></tr>
				<!--#set $oddLine = False#-->
				<!--#for $file in $active_files#-->
					<!--#set $oddLine = not $oddLine#-->
					<tr id="$file.nzf_id" class="nzf_row <!--#if $oddLine then "oddLine" else "evenLine"#--> active">
						<td><input type="checkbox" name="$file.nzf_id" id="box-$file.nzf_id" <!--#if $file.checked then "checked" else ""#-->></td>
						<td>$file.filename</td>
						<td>$file.mb</td>
						<td>$file.age</td>
					</tr>
				<!--#end for#-->
			</table>
		
			<div class="footer">
				<!--#if int($cache_limit)#--><b>$cache_art</b> articles buffered in <b>$cache_size</b> bytes<br><!--#end if#-->
				<!--#if $nzb_quota then "<b>NzbQuota:</b> %s<b>" %($nzb_quota) else ""#-->
			</div>
			
	<!-- ************************************
				PAGE CONTENT IS ABOVE
		************************************ -->
			
		</div>
	</div>
	
	</form>
	
</body>
</html>
