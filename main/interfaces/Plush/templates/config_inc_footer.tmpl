        </div>
    </div>

    #if $pane=="Connections"#
        <div class="left_links config_margin">
            <a class="rounded" id="force_disconnect" href="disconnect">Force Disconnect</a>
        </div>
    #else if $pane=="General" or $pane=="Folders" or $pane=="Switches" or $pane=="Email" or $pane=="NZB Sites" or $pane=="Sorting"#
        <div class="left_links config_margin">
            <a class="rounded" id="save" onclick="document.fullform.submit();">Save Changes</a>
        </div>
    #end if#
    #if $pane=="General"#
        <div class="left_links">
            <a class="rounded" id="sabnzbd_restart">Restart SABnzbd</a>
        </div>
    #end if#
    #if $pane=="Email"#
        <div class="left_links">
            <a class="rounded" id="test_email" href="../../connections/testmail" onclick="javascript:return confirm('This will send a test email to your account.')">Test Email</a>
        </div>
    #end if#

    <div class="right_links">
        <a class="rounded" id="help" target="_blank" href="$helpuri$help_uri">$pane Help</a>
    </div>

</body>
</html>
