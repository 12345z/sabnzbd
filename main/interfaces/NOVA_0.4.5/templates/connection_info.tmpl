<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN">
<html>
<head>
    <title>SABnzbd+ $version - Queued: $mbleft MB</title>
    <link rel="stylesheet" type="text/css" href="../static/stylesheets/default.css?version=$web_name"/>
</head>
<body>
<h1><span id="first">SAB</span><span id="second">nzbd+</span><span id="third"> $version</span><span id="catchfrase">The automatic usenet download tool</span></h1>
<span class="MainMenu">
<a href="../">NOVA</a> |
<a class="current" href="./">Connections</a> |
<a href="../config">Config</a> |
<a href="http://sabnzbdplus.wiki.sourceforge.net/GUI+Connections" target="_blank">Help</a>
</span>
<span class="SubMenu">
<a href="./disconnect">Force Disconnect</a> |
<a href="./testmail" onclick="javascript:return confirm('This will send a test email to your account.')">Test Email</a> |
<a href="./showlog">Show logging</a> |
<a href="./showweb">Show Weblogging</a>
</span>
<h2>Connections</h2>
<!--#if $servers#-->
    <ul>
    <!--#for $server in $servers#-->
        <li>
            $server[0]:$server[1]: $server[2]
            <ul>
            <!--#for $thrd in $server[3]#-->
                <li>Thread #$thrd[0] -> $thrd[1] -> $thrd[2] -> $thrd[3]</li>
            <!--#end for#-->
            </ul>
        </li>
    <!--#end for#-->
    </ul>
<!--#end if#-->
<!--#if $lastmail#-->
    <h2>Email test result</h2>
    $lastmail
<!--#end if#-->
<div class="footer">
<!--#if $shutdown then "<b>SHUTDOWN</b> - " else ""#--><b>Download Dir:</b> $diskspace1 GB - <b>Complete Dir:</b> $diskspace2 GB - <b>Download speed:</b> $kbpersec KB/s - <b>Queued:</b> $mbleft/$mb MB <!--#if $paused then "- <b>PAUSED</b>" else ""#--><br>
<!--#if int($cache_limit)#-->
<b>$cache_art</b> articles buffered in <b>$cache_size</b> bytes<br>
<!--#end if#-->
<!--#if $new_release#-->
    <b>New release $new_release available at <a href="$new_rel_url" target="_blank">SF.net</a></b><br>
<!--#end if#-->
</div>
</body>
</html>
