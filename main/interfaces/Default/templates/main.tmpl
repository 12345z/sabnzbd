<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN">
<html>
<head>
    <title>SABnzbd+ $version - Queued: $mbleft MB</title>
    <link rel="stylesheet" type="text/css" href="static/stylesheets/default.css"/>
    <link rel="stylesheet" type="text/css" href="static/stylesheets/defaultcolors.css"/>
    <!--#if $color_scheme#-->
        <link rel="stylesheet" type="text/css" href="static/stylesheets/colorschemes/$color_scheme"/>
    <!--#end if#-->
    <link rel="shortcut icon" href="static/images/favicon.ico" />
</head>
<body>
<h1><span id="first">SAB</span><span id="second">nzbd+</span><span id="third"> $version</span><span id="catchfrase">The automatic usenet download tool</span></h1>
<span class="MainMenu">
<a class="current" href="./">Home</a> |
<a href="./queue">Queue</a> |
<a href="./history">History</a> |
<a href="./config">Config</a> |
<a href="./connections">Connections</a> |
<a href="$helpuri/GUI+Main" target="_blank">Help</a>
</span>
<span class="SubMenu">
<!--#if $paused then "<a href=\"./resume\">Resume</a>" else "<a href=\"./pause\">Pause</a>"#--> |
<a href="./shutdown" onclick="javascript:return confirm('Are you sure you want to shutdown SABnzbd?')">Shutdown</a>
</span>
<h2>Add new downloads</h2>
<!--#if $varExists('newzbinDetails')#-->
<div class="EntryBlock">
    <form action="addID" method="get">
    <fieldset class="EntryFieldSet">
      <legend>Add Postid</legend>
      <input type="text" name="id">
            <select name="pp">
            <option value="0">None</option>
            <option value="1">+Repair</option>
            <option value="2">+Unpack</option>
            <option value="3" selected>+Delete</option>
            </select>
            <br>
            <input type="submit" value="Add">
      </fieldset>
    </form>
</div>
<!--#end if#-->
<div class="EntryBlock">
    <form action="addURL" method="get">
    <fieldset class="EntryFieldSet">
            <legend>Add URL</legend>
    <input type="text" name="url">
        <!--#if $cat_list#-->
              <select name="cat">
                  <!--#for $ct in $cat_list#-->
                      <option value="$ct" <!--#if "$ct" == "$cat" then "selected" else ""#-->>$ct</option>
                  <!--#end for#-->
              </select>
        <!--#else#-->
              <select name="pp">
              <option value="0">None</option>
              <option value="1">+Repair</option>
              <option value="2">+Unpack</option>
              <option value="3" selected>+Delete</option>
              </select>
        <!--#end if#-->
        <br>
        <input type="submit" value="Add">
    </fieldset>
    </form>
</div>
<div class="EntryBlock">
    <form action="addFile" method="post" enctype="multipart/form-data">
    <fieldset class="EntryFieldSet">
      <legend>Add File</legend>
      <input type="file" name="nzbfile">
      <!--#if $cat_list#-->
            <select name="cat">
                <!--#for $ct in $cat_list#-->
                    <option value="$ct" <!--#if "$ct" == "$cat" then "selected" else ""#-->>$ct</option>
                <!--#end for#-->
            </select>
      <!--#else#-->
            <select name="pp">
            <option value="0">None</option>
            <option value="1">+Repair</option>
            <option value="2">+Unpack</option>
            <option value="3" selected>+Delete</option>
            </select>
      <!--#end if#-->
        <br>
      <input type="submit" value="Add">
    </fieldset>
    </form>
</div>
<div class="footer">
<!--#if $shutdown then "<b>SHUTDOWN</b> - " else ""#--><b>Download Dir:</b> $diskspace1 GB - <b>Complete Dir:</b> $diskspace2 GB - <b>Download speed:</b> $kbpersec KB/s - <b>Queued:</b> $mbleft/$mb MB <!--#if $paused then "- <b>PAUSED</b>" else ""#--><br>
<!--#if int($cache_limit)#-->
<b>$cache_art</b> articles buffered in <b>$cache_size</b> bytes<br>
<!--#end if#-->
<!--#if $new_release#-->
    <b>New release $new_release available at <a href="$new_rel_url" target="_blank">SF.net</a></b><br>
<!--#end if#-->
</div>
<!--#set $kbpersecrnd = str(int(float($kbpersec)))#-->
<!--#set $mbleftrnd = str(int(float($mbleft)))#-->
<!--#set $mbrnd = str(int(float($mb)))#-->
<div id='SABData' style='visibility: hidden'>
    <b>&nbsp;SAB:</b> <!--#if $paused then "<blink><b>PAUSED</b></blink>" else $kbpersecrnd + " KB/s"#--> <b>Q:</b> $mbleftrnd/$mbrnd MB<b>&nbsp; Comp:</b> $diskspace2 GB
</div>
<!--#if $warning#-->
    <h2>WARNING</h2>
    <b>$warning</b>
<!--#end if#-->
</body>
</html>
