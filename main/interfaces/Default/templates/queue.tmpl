<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN">
<html>
<head>
    <title>SABnzbd+ $version - Queued: $mbleft MB</title>
    <link rel="stylesheet" type="text/css" href="../static/stylesheets/default.css"/>
    <link rel="stylesheet" type="text/css" href="../static/stylesheets/defaultcolors.css"/>
    <!--#if $color_scheme#-->
        <link rel="stylesheet" type="text/css" href="../static/stylesheets/colorschemes/$color_scheme"/>
    <!--#end if#-->
    <!--#if $refresh_rate#-->
        <meta http-equiv="refresh" content="$refresh_rate">
    <!--#end if#-->
    <link rel="shortcut icon" href="../static/images/favicon.ico" />
</head>
<body>
<h1><span id="first">SAB</span><span id="second">nzbd+</span><span id="third"> $version</span><span id="catchfrase">The automatic usenet download tool</span></h1>
<span class="MainMenu">
<a href="../">Home</a> |
<a class="current" href="./">Queue</a> |
<a href="../history">History</a> |
<a href="../config">Config</a> |
<a href="../connections">Connections</a> |
<a href="$helpuri/GUI+Queue" target="_blank">Help</a>
</span>
<span class="SubMenu">
<!--#if $paused then "<a href=\"./resume\">Resume</a>" else "<a href=\"./pause\">Pause</a>"#--> |
<a href="./sort_by_name">Sort by name</a> |
<a href="./sort_by_avg_age">Sort by age</a> |
<a href="./tog_verbose">Toggle verbosity</a>
On queue finish:
<select onChange="location = './change_queue_complete_action?action='+this.options[this.selectedIndex].value">
<option value=""></option>
    <optgroup label="Actions">
        <!--#if $nt#-->
        <option value="shutdown_pc" <!--#if $finishaction == 'shutdown_pc' then 'selected' else ''#-->>Shutdown PC</option>
        <option value="hibernate_pc" <!--#if $finishaction == 'hibernate_pc' then 'selected' else ''#-->>Hibernate PC</option>
        <!--#end if#-->
        <option value="shutdown_program" <!--#if $finishaction == 'shutdown_program' then 'selected' else ''#-->>Shutdown SABnzbd</option>
    </optgroup>
<!--#if $script_list#-->
    <optgroup label="Scripts">
    <!--#for $sc in $script_list#-->
        <!--#if $sc != 'None'#-->
        <option value="script_$sc" <!--#if $finishaction == 'script_'+$sc then 'selected' else ''#-->>$sc</option>
        <!--#end if#-->
    <!--#end for#-->
    </optgroup>
<!--#end if#-->
</select>
</span>
<br>
<br>
<!--#if $varExists('slotinfo')#-->
    <table id="queueTable">
        <tr> <th></th><th></th><th></th>
            <!--#if $cat_list#--><th>Category</th><!--#end if#-->
            <th>Mode</th>
            <!--#if $script_list#--><th>Script</th><!--#end if#-->
            <th>Name</th><th>Remain/Total</th><th>ETA</th><th>AGE</th>
        </tr>
    <!--#set $odd = False#-->
    <!--#for $slot in $slotinfo#-->
        <!--#set $odd = not $odd#-->
        <tr class="<!--#if $odd then "odd" else "even"#-->">
        <td><a class="verbosity_link" href="./tog_uid_verbose?uid=$slot.nzo_id"> <!--#if $slot.active then "-" else "+" #--> </a></td>
        <td><form action="delete" method="get">
            <input type="hidden" value="$slot.nzo_id" name="uid">
            <input type="submit" value="Delete"></form>
            <td><form><select onchange="location = this.options[this.selectedIndex].value;">
            <!--#for $i in xrange($noofslots)#-->
                <option value="switch?uid1=$slot.nzo_id&uid2=$slotinfo[i].nzo_id" <!--#if $i == $slot.index then "selected" else ""#-->>$i</option>
            <!--#end for#-->
            </select></form></td>
            <!--#if $cat_list#-->
            <td><form><select onchange="location = this.options[this.selectedIndex].value;">
                <!--#for $ct in $cat_list#-->
                    <option value="change_cat?nzo_id=$slot.nzo_id&cat=$ct" <!--#if $slot.cat.lower() == $ct.lower() then "selected" else ""#-->>$ct</option>
                <!--#end for#-->
                </select></form>
            </td>
            <!--#end if#-->
            <td><form><select onchange="location = this.options[this.selectedIndex].value;">
                <option value="change_opts?nzo_id=$slot.nzo_id&pp=0" <!--#if $slot.unpackopts == "0" then "selected" else ""#-->></option>
                <option value="change_opts?nzo_id=$slot.nzo_id&pp=1" <!--#if $slot.unpackopts == "1" then "selected" else ""#-->>R</option>
                <option value="change_opts?nzo_id=$slot.nzo_id&pp=2" <!--#if $slot.unpackopts == "2" then "selected" else ""#-->>U</option>
                <option value="change_opts?nzo_id=$slot.nzo_id&pp=3" <!--#if $slot.unpackopts == "3" then "selected" else ""#-->>D</option>
                </select></form>
            </td>
            <!--#if $script_list#-->
            <td><form><select onchange="location = this.options[this.selectedIndex].value;">
                <!--#for $sc in $script_list#-->
                    <option value="change_script?nzo_id=$slot.nzo_id&script=$sc" <!--#if $slot.script.lower() == $sc.lower() then "selected" else ""#-->>$sc</option>
                <!--#end for#-->
                </select></form>
            </td>
            <!--#end if#-->
            <td><a href="./$slot.nzo_id/">$slot.filename</a></td>
            <td class="pre">$slot.mbleft/$slot.mb MB</td>
            <td>$slot.eta</td>
            <td>$slot.avg_age</td>
        </tr>
        <!--#set $oddLine = False#-->
        <!--#for $line in $slot.finished#-->
            <!--#set $oddLine = not $oddLine#-->
            <tr class="<!--#if $oddLine then "oddLine" else "evenLine"#--> finished"><td></td><td></td><td></td><td>Finished</td><!--#if $script_list#--><td></td><!--#end if#--><td>$line.filename</td><td class="pre">$line.mbleft/$line.mb MB<td></td><td>$line.age</td></tr>
        <!--#end for#-->
        <!--#set $oddLine = False#-->
        <!--#for $line in $slot.active#-->
            <!--#set $oddLine = not $oddLine#-->
            <tr class="<!--#if $oddLine then "oddLine" else "evenLine"#--> active"><td></td><td><a class="remove" href="./removeNzf?nzo_id=$slot.nzo_id&nzf_id=$line.nzf_id">Remove</a></td><td></td><td>Active</td><!--#if $script_list#--><td></td><!--#end if#--><td>$line.filename</td><td class="pre">$line.mbleft/$line.mb MB</td><td></td><td>$line.age</td></tr>
        <!--#end for#-->
        <!--#set $oddLine = False#-->
        <!--#for $line in $slot.queued#-->
            <!--#set $oddLine = not $oddLine#-->
            <tr class="<!--#if $oddLine then "oddLine" else "evenLine"#--> waiting"><td></td><td></td><td></td><td>Waiting</td><!--#if $script_list#--><td></td><!--#end if#--><td>$line.filename (set: $line.set)</td><td class="pre">$line.mbleft/$line.mb MB<td></td><td>$line.age</td></tr>
        <!--#end for#-->
    <!--#end for#-->
    </table>
<!--#end if#-->
<div class="footer">
<!--#if $shutdown then "<b>SHUTDOWN</b> - " else ""#--><b>Download Dir:</b> $diskspace1 GB - <b>Complete Dir:</b> $diskspace2 GB - <b>Download speed:</b> $kbpersec KB/s - <b>Queued:</b> $mbleft/$mb MB <!--#if $paused then "- <b>PAUSED</b>" else ""#--><br>
<!--#if int($cache_limit)#-->
<b>$cache_art</b> articles buffered in <b>$cache_size</b> bytes<br>
<!--#end if#-->
<!--#if $new_release#-->
    <b>New release $new_release available at <a href="$new_rel_url" target="_blank">SF.net</a></b><br>
<!--#end if#-->
</div>
</body>
</html>
