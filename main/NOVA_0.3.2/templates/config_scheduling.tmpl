<!-- ****************************************** BEGIN CONTENT ****************************************** -->
			
			
			<!-- Begin Sub Menu -->
			<span class="subMenu">
				<a href="../../config/general">General</a><a href="../../config/directories">Directories</a><a href="../../config/switches">Switches</a><a href="../../config/server">Servers</a><a href="../../config/scheduling" class="current">Scheduling</a><a href="../../config/rss">RSS</a><a href="../../config/email">Email</a>
			</span>
			<!-- End Sub Menu -->




		<h2>Scheduling configuration</h2>
			<div class="EntryBlock">
				<form action="addSchedule" method="post">
					<fieldset class="EntryFieldSet">
						<legend>Add schedule</legend>
						<select name="minute">
							<!--#for $i in range(60)#-->
							<option value=$i>$i
							<!--#end for#-->
						</select>
						<select name="hour">
							<!--#for $i in range(24)#-->
							<option value=$i>$i
							<!--#end for#-->
						</select>
						<select name="dayofweek">
							<option value="*" selected>*
							<!--#for $i in range(1, 8)#-->
							<option value=$i>$i
							<!--#end for#-->
						</select>
						<select name="action">
							<option value="resume">resume
							<option value="pause">pause
						</select>
						<p><input type="submit" value="Add Schedule"></p>
					</fieldset>
				</form>
			</div>



		<h2>Current Schedules:</h2>
			<div class="EntryBlock>"
				<!--#set $schednum = 0#-->
				<!--#for $line in $schedlines#-->
				<form action="delSchedule" method="post">
					<fieldset class="EntryFieldSet">
						<!--#set $schednum = $schednum+1#-->
						<legend>Task $schednum</legend>
						$line
						<input type="hidden" name="line" value="$line">
						<input type="submit" value="Remove">
					</fieldset>
				</form>
				<!--#end for#-->
			</div>



<!-- ****************************************** END CONTENT ****************************************** -->
