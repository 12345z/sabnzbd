SABNZBDJSON
{
	"uptime": "$uptime",
	"freedown": $diskspace1,
	"freecomp": $diskspace2,
	"speed": $kbpersec,
	"left": $mbleft,
	"total": $mb,
	"paused": "$paused",
	"shutdown": "$shutdown",
	
	"jobs": [
	<!--#if $varExists('slotinfo')#-->
	<!--#for $slot in $slotinfo#-->
	{"id": "$slot.nzo_id", "name": "$slot.filename", "left": $slot.mbleft, "total": $slot.mb, "eta": "$slot.eta", "age": "$slot.avg_age"},
	
	<!--#end for#-->
	<!--#end if#-->]
}
-->


</body>
</html>